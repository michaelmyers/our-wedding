{
  "name": "wedding-website",
  "version": "0.0.1",
  "description": "A wedding website",
  "main": "index.js",
  "scripts": {
    "postinstall": "typings install",
    "lint": "tslint 'src/**/*.{ts,tsx}' -e **/*.scss.d.ts",
    "pretest": "tsc && npm run lint",
    "precoverage": "npm run pretest",
    "coverage": "istanbul cover --include-all-sources --root src/ -x \"**/*test.js\" _mocha \"src/**/*.test.js\" -- --require ignore-styles -R spec && remap-istanbul -i ./coverage/coverage.json -o ./coverage/ts-report -t html && ./node_modules/.bin/remap-istanbul -i coverage/coverage.json -t lcovonly > coverage/lcov-ts.info",
    "coveralls": "npm run coverage && ./node_modules/coveralls/bin/coveralls.js < coverage/lcov-ts.info",
    "test": "mocha 'src/**/*.test.js'",
    "start": "node server",
    "build": "NODE_ENV=production webpack",
    "predeploy": "npm run build",
    "deploy": "node bin deploy"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.17.5",
    "bs-fullscreen-message": "^1.1.0",
    "chai": "^3.5.0",
    "commander": "^2.9.0",
    "connect-history-api-fallback": "^1.3.0",
    "coveralls": "^2.11.14",
    "css-loader": "^0.25.0",
    "csv-parse": "^1.1.7",
    "del": "^2.2.2",
    "enzyme": "^2.5.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-mock": "^5.5.0",
    "firebase": "^3.5.2",
    "firebase-admin": "^4.0.5",
    "firebase-tools": "^3.2.1",
    "ignore-styles": "^5.0.1",
    "immutability-helper": "^2.0.0",
    "istanbul": "^0.4.5",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.2",
    "mocha-jsdom": "^1.1.0",
    "node-sass": "^3.13.0",
    "postcss-loader": "^0.13.0",
    "react-addons-css-transition-group": "^15.4.0",
    "react-addons-test-utils": "^15.4.0",
    "redux-mock-store": "^1.2.1",
    "remap-istanbul": "^0.7.0",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "source-map-loader": "^0.1.5",
    "strip-ansi": "^3.0.1",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.9.5",
    "tslint": "^4.1.1",
    "tslint-react": "^2.2.0",
    "typescript": "^2.1.4",
    "typings": "^2.1.0",
    "typings-for-css-modules-loader": "^1.0.0",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "blueimp-md5": "^2.6.0",
    "classnames": "^2.2.5",
    "es6-promise": "^4.0.5",
    "history": "^3.2.1",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.15.2",
    "object-assign": "^4.1.0",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-ga": "^2.1.2",
    "react-json-tree": "^0.10.0",
    "react-list": "^0.8.0",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
